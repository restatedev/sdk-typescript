{
  "name": "@restatedev/deno",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run _build --filter={.}...",
    "_build": "deno install && deno task build",
    "dev": "deno task dev",
    "start": "turbo run _start --filter={.}...",
    "_start": "deno task start",
    "clean": "rm -rf dist *.tsbuildinfo .turbo node_modules",
    "check:types": "turbo run _check:types --filter={.}...",
    "_check:types": "deno install && deno task check",
    "lint": "deno task lint"
  },
  "dependencies": {},
  "devDependencies": {}
}
